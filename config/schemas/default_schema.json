{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "default_schema.json",
  "title": "Default Document Processing Results Schema",
  "type": "object",
  "properties": {
    "metadata": {
      "$ref": "base_types.json#/definitions/ReportMetadata"
    },
    "extraction_info": {
      "$ref": "base_types.json#/definitions/ExtractionInfo"
    },
    "results": {
      "type": "object",
      "properties": {
        "summary": {
          "type": "string",
          "description": "Brief summary of the processing report",
          "maxLength": 1000
        },
        "total_files_processed": {
          "type": "integer",
          "minimum": 0
        },
        "file_types_found": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "key_findings": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "category": {
                "type": "string",
                "enum": ["document", "image", "contact", "data", "error", "other"]
              },
              "description": {
                "type": "string"
              },
              "importance": {
                "type": "string",
                "enum": ["high", "medium", "low"]
              }
            },
            "required": ["category", "description"]
          }
        },
        "processing_errors": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "file_name": {
                "type": "string"
              },
              "error_type": {
                "type": "string"
              },
              "error_description": {
                "type": "string"
              }
            },
            "required": ["file_name", "error_type"]
          }
        }
      },
      "required": ["summary", "total_files_processed"]
    }
  },
  "required": ["metadata", "results"]
}